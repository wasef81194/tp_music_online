<form method="post" action="{{ path('app_admin_album_delete', {'id': album.id}) }}" onsubmit="return confirm('Voulez vous vraiment supprimer cette album et ces musiques?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ album.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
